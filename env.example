# Contentful Configuration
# Copy this file to .env and fill in your actual values

# Space 1 - BlockHomeHeroSlider (Main stories content)
EXPO_PUBLIC_SPACE1_TOKEN=your_space1_access_token_here

# Space 2 - Announcements (Banner content)  
EXPO_PUBLIC_SPACE2_TOKEN=your_space2_access_token_here

# Content ID for the hero slider content type
EXPO_PUBLIC_HERO_SLIDER_CONTENT_ID=your_hero_slider_content_id_here

# Base URL for external links (used in banner CTA links)
EXPO_PUBLIC_BASE_SITE_URL=https://www.pmg.com

# Example values (replace with your actual Contentful credentials):
# EXPO_PUBLIC_SPACE1_TOKEN=CFPAT-abc123def456ghi789jkl012mno345pqr678stu901vwx234yz
# EXPO_PUBLIC_SPACE2_TOKEN=CFPAT-xyz987wvu654tsr321qpo098nml765kji432hgf109edc876ba
# EXPO_PUBLIC_HERO_SLIDER_CONTENT_ID=blockHomeHeroSlider
# EXPO_PUBLIC_BASE_SITE_URL=https://www.pmg.com 